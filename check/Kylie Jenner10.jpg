<script>
function startTime() {
    var today = new Date();
var h = today.getHours() - (today.getHours() >= 12 ? 12 : 0);
var period = today.getHours() >= 12 ? 'PM' : 'AM';
    var m = today.getMinutes();
    var s = today.getSeconds();
    m = checkTime(m);
    s = checkTime(s);
    if (h == 0) {
        h = 12;
    }
    document.getElementById('tg-time').innerHTML =
    "// " + h + ":" + m + " " +period;
    var t = setTimeout(startTime, 500);
}

function checkTime(i) {
    if (i < 10) {i = "0" + i};  // add zero in front of numbers < 10
    return i;
}
</script>

